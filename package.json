{
  "name": "@ryaninvents/changeset-run",
  "description": "Use custom publish scripts for packages in Atlassian Changesets, perfect for non-npm artifacts",
  "version": "0.0.0",
  "author": "Ryan Kennedy",
  "bin": {
    "changeset-run": "./bin/run"
  },
  "bugs": "https://github.com/ryaninvents/changeset-run/issues",
  "dependencies": {
    "@changesets/cli": "^2.6.5",
    "@manypkg/get-packages": "^1.0.1",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "chalk": "^4.0.0",
    "execa": "^4.0.0",
    "has-yarn": "^2.1.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@changesets/types": "^2.0.1",
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=10.1.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ryaninvents/changeset-run",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "changeset-run",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "ryaninvents/changeset-run",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
